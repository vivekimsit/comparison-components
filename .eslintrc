{
  "env": {
    "browser": true,
    "jest": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": true}],
  }
}
