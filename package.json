{
  "name": "comparison-components",
  "version": "2.0.1",
  "description": "Comparison components to be used in JS pluggable widget, Angular component etc.",
  "main": "dist/comparison-components.js",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --content-base demo/ --port 4040 --progress --inline --hot",
    "build": "webpack -p",
    "lint": "node_modules/.bin/eslint ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "bootstrap": "github:transferwise/bootstrap#v4.1.11",
    "css-loader": "^0.23.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "graceful-fs": "^4.1.9",
    "jest": "^17.0.3",
    "jest-enzyme": "^2.0.0",
    "jest-fetch-mock": "^1.0.6",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "^15.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./.jestrc"
    ]
  }
